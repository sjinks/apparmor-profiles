#include <tunables/global>

/usr/sbin/haveged {
  #include <abstractions/base>

  capability sys_admin,

  /proc/*/status r,
  /proc/sys/kernel/osrelease r,
  /proc/sys/kernel/random/poolsize r,
  /sys/devices/system/cpu/ r,
  /sys/devices/system/cpu/** r,
  /usr/sbin/haveged mr,

  #include <local/usr/sbin/haveged>
}
